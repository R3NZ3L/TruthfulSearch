{
  "name": "Thesis Extension",
  "description": "removes the recommendation section of the youtube watch page",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "js": ["script.js"],
      "css": ["InfoCard.css"],
      "matches": [
        "https://www.youtube.com/watch?v=*",
        "https://youtube.com/watch?v=*",
        "https://youtube.com/watch/*",
        "https://*.youtube.com/watch?v=*"
      ]
    }
  ]
}